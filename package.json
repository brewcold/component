{
  "name": "@syyu/component",
  "version": "1.0.0",
  "description": "basic react component system for boilerplate",
  "keywords": [
    "component",
    "react",
    "component system"
  ],
  "scripts": {
    "build": "yarn dlx jest && rollup --config",
    "test": "yarn dlx jest --watchAll --verbose",
    "publish": "npm publish"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:brewcold/components.git",
  "author": "brewcold <seung.yoon.yu@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@4.5.0",
  "files": [
    "dist/",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/cjs/index.d.ts"
      }
    }
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@emotion/babel-plugin": "^11.12.0",
    "@emotion/react": "^11.13.3",
    "@eslint/js": "^9.11.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.12",
    "@swc/jest": "^0.2.36",
    "@syyu/util": "^1.3.9",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/jest": "29.5.12",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-test-renderer": "^18",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "axios": "^1.7.7",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "globals": "^15.9.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nock": "^13.5.5",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "rollup": "^4.22.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.8.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "arrowParens": "avoid",
    "printWidth": 120
  }
}
